<!-- FullScreen -->
{#<ol>
    {% for key, value in _context  %}
        <li>{{ key }}</li>
    {% endfor %}
</ol>#}
<div class="intro-header">
    <div class="col-xs-12 text-center abcen1">
        <h1 class="h1_home wow fadeIn" data-wow-delay="0.4s">{{ locale['block.fullscreen.title'] }}</h1>
        {% autoescape false %}
        <h3 class="h3_home wow fadeIn" data-wow-delay="0.6s">{{ locale['block.fullscreen.desc'] }}</h3>
        {% endautoescape %}
        <ul class="list-inline intro-social-buttons">
            <li>
                <a href="{{ locale['block.fullscreen.repost.vk.url'] }}" target="_blank" class="btn  btn-lg mybutton_cyano wow fadeIn" data-wow-delay="0.8s"><span class="network-name">{{ locale['block.fullscreen.repost.vk'] }}</span></a>
            </li>
            <li id="download">
                <a href="{{ locale['block.fullscreen.repost.fb.url'] }}" target="_blank" class="btn  btn-lg mybutton_standard wow swing wow fadeIn" data-wow-delay="1.2s"><span class="network-name">{{ locale['block.fullscreen.repost.fb'] }}</span></a>
            </li>

        </ul>
    </div>

    <!-- /.container -->
    <div class="col-xs-12 text-center abcen wow fadeIn">
        <div class="button_down ">
            <a class="imgcircle wow bounceInUp" data-wow-duration="1.5s"  href="#whatis"> <img class="img_scroll" src="img/icon/circle.png" alt=""> </a>
        </div>
    </div>
</div>